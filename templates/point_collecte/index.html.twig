{% extends 'base.html.twig' %}

{% block title %}PointCollecte index{% endblock %}

{% block body %}
    <h1>PointCollecte index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>Adresse</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for point_collecte in point_collectes %}
            <tr>
                <td>{{ point_collecte.id }}</td>
                <td>{{ point_collecte.type }}</td>
                <td>{{ point_collecte.adresse }}</td>
                <td>
                    <a href="{{ path('app_point_collecte_show', {'id': point_collecte.id}) }}">show</a>
                    <a href="{{ path('app_point_collecte_edit', {'id': point_collecte.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_point_collecte_new') }}">Create new</a>
{% endblock %}
